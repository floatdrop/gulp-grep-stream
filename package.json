{
  "name": "gulp-grep-stream",
  "version": "0.0.1",
  "description": "A gulp file stream filtering plugin for Gulp",
  "keywords": [
    "gulpplugin",
    "grep",
    "filter",
    "ignore",
    "include",
    "exclude"
  ],
  "homepage": "https://github.com/floatdrop/gulp-grep-stream",
  "bugs": "https://github.com/floatdrop/gulp-grep/issues",
  "author": {
    "name": "Vsevolod Strukchinsky",
    "email": "floatdrop@gmail.com",
    "url": "https://github.com/floatdrop"
  },
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/floatdrop/gulp-grep-stream.git"
  },
  "scripts": {
    "test": "istanbul test _mocha --report html -- test/*.js --reporter spec",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "dependencies": {
    "through": "~2.3.4",
    "gulp-util": "~2.2.9",
    "minimatch": "~0.2.14"
  },
  "devDependencies": {
    "mocha": "*",
    "coveralls": "*",
    "mocha-lcov-reporter": "*",
    "istanbul": "*",
    "event-stream": "~3.0.20",
    "gulp-mocha": "*",
    "gulp-watch": "*",
    "chai": "*"
  },
  "engines": {
    "node": ">=0.8.0",
    "npm": ">=1.2.10"
  },
  "licenses": [
    {
      "type": "MIT"
    }
  ]
}
